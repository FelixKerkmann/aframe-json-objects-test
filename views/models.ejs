<!DOCTYPE html>
<html>
<head>
    <script src="https://aframe.io/releases/1.3.0/aframe.min.js"></script>
    <style>
        div {
            padding: 20px;
            float: left;
            width: 1100px;
        }
        input {
            width: 50px;
        }
        .header li {
            float: left;
            padding: 5px;
            margin: 5px;
            text-align: center;
            height: 20px;
            width: 58px;
        }
        h3 {
            margin: 0px;
            width: 200px;
        }
        ul {
            border: 1px solid darkgray;
            list-style-type: none;
            margin: 0;
            padding: 0;
            overflow: hidden;
        }
        .model li {
            float: left;
            padding: 5px;
            margin: 5px;
            text-align: center;
            height: 20px;
            width: auto;
        }
    </style>
</head>
<body>
<div>
    <ul>
        <li>
            <ul class="header">
                <li style="width: 200px"><h3>fname</h3></li>
                <li>PosX</li>
                <li>PosY</li>
                <li>PosZ</li>
                <li>RotX</li>
                <li>RotY</li>
                <li>RotZ</li>
                <li>Scale</li>
            </ul>
        </li>
        <%- models %>
    </ul>
</div>
<div>
    <a href="/form">Upload 3D Model</a>
    <br>
    <a href="/">Back to Scene</a>
</div>
</body>
</html>